<div class="row">
	<div class="col-xs-12">

		<h2>Follows users</h2>

		<% @user.followings.each do |user| %>
			<table>
				<tr>
					<th>image</th>
					<th>name</th>
					<th colspan="3"></th>
				</tr>
			</table>
			<thead>
				<tr>
					<td><%= attachment_image_tag(user, :profile_image, :fill, 40, 40, fallback: "no-image-mini.jpg") %></td>
					<td><%= user.name %></td>
					<td>フォロー数：<%= @user.followings.count %></td>
					<td>フォロワー数：<%= @user.followers.count %></td>
				</tr>
			</thead>


</table>
<% end %>
</div>
</div>